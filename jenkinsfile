pipeline {
	agent any
		stages {
			stage('Build') {
                      	steps {
								echo 'Running build phase..'
								git 'https://github.com/msg2fahd/pipeline.git'
                                sh './build.sh'
                           	}
                    	}
         	
         	stage('Test'){
         				steps{
         						echo 'Testing progress...'
         					}
        			 	}
         	
         	stage('Deploy'){
         				steps{
         						echo 'Deploying is in  progress...'
         					}
         				}

         }        
 }
